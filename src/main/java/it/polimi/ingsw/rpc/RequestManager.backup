package it.polimi.ingsw.rpc;

import java.io.IOException;
import java.io.ObjectInputStream;
import java.io.ObjectOutputStream;
import java.net.ServerSocket;
import java.net.Socket;
import java.util.HashMap;

public class RequestManager {
    Socket socket;

    public void f(ServerSocket server) throws IOException {

        var counter = 0;
        var s = server.accept();
        var in = new ObjectInputStream(s.getInputStream());
        var out = new ObjectOutputStream(s.getOutputStream());
        out.writeInt(counter);


        server.accept();
    }
}

class TemporaryStorage implements AutoCloseable {
    Socket socket;
    ObjectInputStream input;
    ObjectOutputStream output;

    public TemporaryStorage(Socket socket) {
        this.socket = socket;
    }

    @Override
    public void close() throws Exception {
        socket.close();
        input.close();
        output.close();
    }
}